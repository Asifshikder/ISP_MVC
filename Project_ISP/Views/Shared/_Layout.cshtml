        //    }
        //},
        function addRequestVerificationTokenLayout(data) {

            data.__RequestVerificationToken = $('input[name=__RequestVerificationToken]').val();
            return data;
        }
        function GetlayoutClientLineHistoryByCID(CID) {
            var url = "/client/LineStatusChangeHistoryByClientDetailsID/";
            var Data = ({ SearchID: CID });
            Data = addRequestVerificationTokenLayout(Data);
            AppUtil.MakeAjaxCallsForAntiForgery(url, "POST", Data, GetlayoutClientLineHistoryByCIDSuccess, GetlayoutClientLineHistoryByCIDFail);
        }
        function GetlayoutClientLineHistoryByCIDSuccess(data) {
            if (data.Success === true) {
                $.each(data.lstClientLineStatus, function (index, item) {


                    var status = "";
                    if (item.Status == 3) {
                        status = "<div id='status' class='label label-success'>Active</div>"
                    }
                    if (item.Status == 4) {
                        status = "<div id='status' class='label label-warning'>In Active</div>"
                    }
                    if (item.Status == 5) {
                        status = "<div id='status' class='label label-danger'>Lock</div>"
                    }
                    $("#tblLayoutClientChangeHistory>tbody").append("<tr><td>" + item.Name + "</td><td>" + item.LoginName + "</td><td>" + item.Package + "</td><td>" + item.Address + "</td>\
                                                                                                    <td>" + item.Zone + "</td><td>" + item.Contact + "</td><td>" + status + "</td><td>" + item.Employee + "</td>\
                                                                                                    <td>" + AppUtil.ParseDateTime(item.Time) + "</td><td>" + item.Reason + "</td></tr>");
                });
                $("#mdlLayoutClientChangeHistory").modal("show");
            }
        }
        function GetlayoutClientLineHistoryByCIDFail(data) {
            if (data.Success === false) {
                AppUtil.ShowError("Something is wrong. Contact with administrator.")
            }
        }
    </script>

