        $(document).ready(function () {
            $("#search-btn").keyup(function () {

                var contentLength = $("#search-btn").val().length;
                if (contentLength > 0) {
                    var keyword = $("#search-btn").val();
                    $("#tblAllClientsDashBoard>tbody").empty();
                    SearchClientInformationManager.GetClientInformationBySearchKeyWord(keyword);

                    $("#ShowInformation").css("display", "block");
                } else {
                    $("#tblAllClientsDashBoard>tbody").empty();
                    $("#ShowInformation").css("display", "none");
                }
            });

            $("#tblAllClientsDashBoard").on("click",
                "#showClientFromDashBoard",
                function () {

                    var achID = $(this).closest("tr").find("td:eq(0) input").val();
                    ClientUpdateFromDashBoardManager.GetClientDetailsByIDDashBoard(achID);
                });

            $("#PopUpDashBoard").on("click",
                "#btnUpdateClientDetailsDashBoard",
                function () {

                    if (ClientUpdateFromDashBoardManager.UpdateClientDetailsValidationFromDashBoard() === true) {
                        //ClientUpdataeFromSeveralPageManager.UpdateClientDetailsPopUpFromOtherPages();
                        ClientUpdateFromDashBoardManager.UpdateClientDetailsFromDashBoard();
                    }

                });

            $("#tblAllClientsDashBoard").on("click", "#layoutClientStatusUpdateHistory", function () {

                var cID = $(this).closest("tr").find("td:eq(0) input").val();
                GetlayoutClientLineHistoryByCID(cID);

            });

            $("#btnPopUptblLayoutClientChangeHistoryClose,#closetblLayoutClientChangeHistory").click(function () {
                $("#tblLayoutClientChangeHistory>tbody").empty();
                $("#mdlLayoutClientChangeHistory").modal("hide");
            });
            $("#closeLayoutShowInformation").click(function () {
                $("#tblAllClientsDashBoard>tbody").empty();
                $("#ShowInformation").css("display", "none");
                $("#search-btn").val("");
            });
        });

