<div class="form-group col-sm-12">
    <div>
        @Html.Label("ComplainTypeName", null, new { @class = "control-label col-md-4", Style = "float:left" })
        <div class="col-md-8" style="padding-left: 0px;float:left">
            @Html.TextBox("ComplainTypeNames", null, new { @class = "form-control input-sm col-sm-11", @placeholder = " ComplainType Name " })
        </div>
    </div>

    <div class="form-group col-sm-12">
        <div>
            @Html.Label("Show Message Box ", new { @class = "control-label col-md-4", Style = "float:left" })
            <div class="col-md-8" style="padding-left: 0px;">
                @Html.CheckBox("chkMessageBoxUpdate", false)
            </div>
        </div>
    </div>
</div>
