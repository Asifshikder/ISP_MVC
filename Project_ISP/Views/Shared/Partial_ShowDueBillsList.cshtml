

<div class="panel panel-default">
    <div class="panel-heading">
        <h4 class="panel-title">Client Details</h4>
    </div>
    <div class="panel-body">

        <div class="row col-sm-12">
            <div class="form-group col-sm-4">
                <div>
                    @Html.Label("Client Name : ", new { @class = "control-label col-md-0", Style = "float:left" })
                </div>
                @*<div style="clear:both"> </div>*@
                <div class="col-md-8" style="padding-left: 0px;">
                    <div id="dueBillName" style="margin-left:10px; font-weight:bold;"></div>
                </div>
            </div>
            <div class="form-group col-sm-4">
                <div>
                    @Html.Label("Client Login Name : ", new { @class = "control-label col-md-0", Style = "float:left" })
                </div>
                @*<div style="clear:both"> </div>*@
                <div class="col-md-7" style="padding-left: 0px;">
                    <div id="dueBillLoginName" style="margin-left:10px; font-weight:bold;"></div>
                </div>
            </div>
        </div>

        <div class="row col-sm-12">
            <div class="form-group col-sm-4">
                <div>
                    @Html.Label("Client Zone : ", new { @class = "control-label col-md-0", Style = "float:left" })
                </div>
                @*<div style="clear:both"> </div>*@
                <div class="col-md-7" style="padding-left: 0px;">
                    <div id="dueBillClientZone" style="margin-left:10px; font-weight:bold;"></div>
                </div>
            </div>
            <div class="form-group col-sm-8">
                <div>
                    @Html.Label("Client Address : ", new { @class = "control-label col-md-0", Style = "float:left" })
                </div>
                @*<div style="clear:both"> </div>*@
                <div class="col-md-10" style="padding-left: 0px;">
                    <div id="dueBillClientAddress" style="margin-left:10px; font-weight:bold;"></div>
                </div>
            </div>
        </div>

        <div class="row col-sm-12">
            <div class="form-group col-sm-4">
                <div>
                    @Html.Label("Connection Type : ", new { @class = "control-label col-md-0", Style = "float:left" })
                </div>
                @*<div style="clear:both"> </div>*@
                <div class="col-md-7" style="padding-left: 0px;">
                    <div id="dueBillConnectionType" style="margin-left:10px; font-weight:bold;"></div>
                </div>
            </div>
            <div class="form-group col-sm-4">
                <div>
                    @Html.Label("Contact Number : ", new { @class = "control-label col-md-0", Style = "float:left" })
                </div>
                @*<div style="clear:both"> </div>*@
                <div class="col-md-7" style="padding-left: 0px;">
                    <div id="dueBillContactNumber" style="margin-left:10px; font-weight:bold;"></div>
                </div>
            </div>
        </div>

    </div>
</div>





<table id="tblShowDueBillsList" class="table table-hover table-responsive table-striped">
    <thead>
        <tr>
            <th>
                <input type="button" class="btn btn-primary btn-sm" id="btnShowDueBillPaymentWindow" style="visibility:hidden" value="Due Bill Pay" />
            </th>
            <th></th>
            <th>Month</th>
            <th>package</th>
            <th>Fee</th>
            <th>Paid_Amount</th>
            <th>Discount</th>
            <th>Due</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody></tbody>
</table>
